<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class name="com.krishagni.catissueplus.core.dashboard.domain.Dashlet" table="OS_DASHLETS">
    <id name="id" column="IDENTIFIER">
      <generator class="native">
        <param name="sequence">OS_DASHLETS_SEQ</param>
      </generator>
    </id>

    <many-to-one name="dashboard" column="DASHBOARD_ID"/>

    <many-to-one name="dashletConfig" column="DASHLET_CFG_ID"/>

    <property name="height" column="HEIGHT"/>

    <property name="width" column="WIDTH"/>

    <property name="row" column="ROWN"/>

    <property name="column" column="COLN"/>
  </class>
</hibernate-mapping>